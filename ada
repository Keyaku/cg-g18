// Define wheel Blades
// Top Blade (K 25, C 13, C' 14, D 15, D' 16)
[13, 15, 25],
[15, 16, 25],
[16, 14, 25],
[13, 14, 25]
// Right Blade (L 26, E 13, E' 14, F 15, F' 16)
// Bottom Blade (M 27, G 17, G' 18, H 19, H' 20)
// Left Blade (N 28, I 21, I' 22, J 23, J' 24)

[ 0  , wr, hD],       // 25  (K)
[ wr , 0 , hD],       // 26  (L)
[ 0  ,-wr, hD],       // 27  (M)
[-wr , 0 , hD]        // 28  (N)
